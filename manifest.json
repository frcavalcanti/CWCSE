{
  "name": "Test",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
      "service_worker": "background.js"
  },
  "action": {
      "default_popup": "home.html"
  },
  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
  ],
  "permissions": ["tabs", "storage","activeTab","declarativeContent", "clipboardRead"]
}
